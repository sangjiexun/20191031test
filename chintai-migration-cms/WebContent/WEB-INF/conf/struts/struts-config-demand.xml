  <action-mappings>
    <!-- ■店舗コード紐付 -->
    <action path="/demand/ShopBindSearchPage"
            type="net.chintai.backend.sysadmin.demand.action.ShopBindSearchPageAction"
            name="ShopBindSearchPageForm"
            validate="false"
            scope="request">
      <forward name="success" path="/WEB-INF/jsp/demand/ShopBindSearch.jsp" />
    </action>

    <!-- 店舗コード紐付:検索 -->
    <action path="/demand/ShopBindSearch"
            type="net.chintai.backend.sysadmin.demand.action.ShopBindSearchAction"
            scope="request"
            name="ShopBindSearchForm"
            validate="true"
            input="/demand/ShopBindSearchPage.do">
      <forward name="success" path="/WEB-INF/jsp/demand/ShopBindSearch.jsp" />
    </action>

    <!-- 店舗コード紐付:ページング -->
